$(document).ready(function(){var n=function(){var n=this;n.pageModel=ko.observable({orderedBy:ko.observable(""),orderDir:ko.observable("Desc"),pageData:ko.observable(),moversLoading:ko.observable(!0),selectedNews:ko.observable(),selectedInstrument:ko.observable(),selectedIndex:ko.observable(),selectNewsArticle:function(t){n.pageModel().selectedNews(t)},selectInstrument:function(t){n.pageModel().selectedInstrument(t);$("#instrumentModal").modal({show:!0})},selectIndex:function(t){n.pageModel().selectedIndex(t);$("#indexModal").modal({show:!0})},myMoversData:ko.observableArray([]),marketMoversData:ko.observableArray([]),myNewsData:ko.observableArray([])});n.init=function(){n.pageModel().pageData(ko.mapping.fromJS(pageData));ko.applyBindings(n.pageModel,document.getElementById("PageData"));$(".navbar").each(function(){ko.applyBindings(n.pageModel,$(this).find("#PageTitle")[0])});$("#smallChangeAccount").is(":visible")&&ko.applyBindings(n.pageModel,document.getElementById("smallChangeAccount"));svs.ViewModel=n;n.getMovers();$(".navbar-brand").attr("href","#");$(".navbar-brand").attr("style","cursor: default");$("#newsTabContainer a:first").tab("show");$("#indexTabContainer a:first").tab("show");$("#moversTabContainer a:first").tab("show")};n.getMovers=function(){$.ajax({type:"POST",url:window.servicePath+"Home/GetFeedInformation",dataType:"json",contentType:"application/json; charset=utf-8"}).done(n.getMovers_Success)};n.getMovers_Success=function(t){t.Status==!0&&(n.pageModel().moversLoading(!1),t.MyMovers&&$.each(t.MyMovers,function(i){n.pageModel().myMoversData.push(t.MyMovers[i])}),t.MarketMovers&&$.each(t.MarketMovers,function(i){n.pageModel().marketMoversData.push(t.MarketMovers[i])}),t.MyNews&&$.each(t.MyNews,function(i){n.pageModel().myNewsData.push(ko.mapping.fromJS(t.MyNews[i]))}))}};svs.AccountSummary=new n;svs.AccountSummary.init()})